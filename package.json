{
  "name": "react-motion",
  "version": "0.5.2",
  "description": "A spring that solves your animation problems.",
  "main": "lib/react-motion.js",
  "module": "es/react-motion.js",
  "files": [
    "lib",
    "es",
    "build",
    "bower.json",
    "AUTHORS"
  ],
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.34",
    "@babel/core": "7.0.0-beta.34",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.34",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.34",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.34",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.34",
    "@babel/polyfill": "7.0.0-beta.34",
    "@babel/preset-env": "7.0.0-beta.34",
    "@babel/preset-flow": "7.0.0-beta.34",
    "@babel/preset-react": "7.0.0-beta.34",
    "babel-eslint": "^8.0.3",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "codemirror": "^5.5.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.53.1",
    "flow-copy-source": "^1.1.0",
    "inject-loader": "^3.0.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-diff-reporter": "^0.3.2",
    "karma-webpack": "^2.0.6",
    "lodash.range": "^3.0.1",
    "npm-run-all": "^4.1.2",
    "puppeteer": "^0.13.0",
    "react": ">=15.5.0",
    "react-codemirror": ">=0.1.2",
    "react-dom": ">=15.5.0",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "4.0.0-beta.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "scripts": {
    "start": "node server.js",
    "prebuild": "rimraf lib es build",
    "build": "run-p build:**",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore Types.js && flow-copy-source -v src es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore Types.js && flow-copy-source -v src lib",
    "build:umd": "rollup -c -i src/react-motion.js -o build/react-motion.js",
    "build-demos": "webpack",
    "lint": "eslint --ext .js,.jsx .",
    "flow_check": "flow check",
    "prepublish": "npm run build",
    "test": "karma start ./karma.conf.js --single-run",
    "test:travis": "karma start ./karma.conf.js --single-run",
    "test:dev": "karma start ./karma.conf.js --no-single-run --auto-watch",
    "test:cov": "karma start ./karma.conf.js --single-run --reporters coverage",
    "gh-pages": "git fetch origin && git checkout gh-pages && git reset --hard origin/gh-pages && git rebase origin/master --force-rebase && npm run build-demos && git add . && git commit --amend --no-edit && git push origin gh-pages --force && git checkout master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chenglou/react-motion.git"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "transitiongroup",
    "spring",
    "tween",
    "motion",
    "animation",
    "transition",
    "ui"
  ],
  "author": [
    "nkbt",
    "chenglou"
  ],
  "license": "MIT",
  "dependencies": {
    "performance-now": "^0.2.0",
    "prop-types": "^15.6.0",
    "raf": "^3.1.0"
  }
}
